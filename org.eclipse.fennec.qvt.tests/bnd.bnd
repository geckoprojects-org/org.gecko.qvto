-library: enableOSGi-Test,\
	enable-emf

# sets the usually required buildpath using the bnd library, you can extend it with the normal -buildpath to your liking
-buildpath: \
	org.osgi.framework;version=latest,\
	org.osgi.service.cm;version=latest,\
	org.osgi.util.tracker;version=latest,\
	org.gecko.util.pool;version=latest,\
	org.osgi.test.junit5.cm;version=latest,\
	org.eclipse.m2m.qvt.oml;version=latest,\
	org.osgi.service.component;version=latest,\
	org.gecko.emf.osgi.example.model.basic;version=latest,\
	org.eclipse.fennec.qvt.api;version=snapshot,\
	org.eclipse.fennec.qvt.component;version=snapshot,\
	org.apache.aries.typedevent.bus,\
	org.osgi.service.typedevent,\
	org.eclipse.fennec.qvt.event,\
	org.osgi.util.promise,\
	org.osgi.util.function
	
Bundle-Version: 2.0.1.SNAPSHOT

-includeresource: transforms/

-baseline: nope
	
-workingset: qvt